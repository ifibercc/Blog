#footer {
	font-size: 12px;
	font-family: Menlo, Monaco, "Andale Mono", "lucida console", "Courier New", monospace;
	text-shadow: 0 1px #fff;
	position: absolute;
	bottom: 10px;
	opacity: 0.6;
	width: 100%;
	text-align: center;
	.outer{
		padding: 0 30px;
	}
}
.footer-left{
  float: left;
  width: 25%;
	text-align: left;
}
.footer-center{
	float: left;
  width: 50%;
}
.footer-right{
  float: left;
  width: 25%;
	text-align: right;
}